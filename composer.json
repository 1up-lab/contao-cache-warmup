{
  "name":"oneup/contao-page-cache-warmup",
  "description":"This Contao Open Source CMS extension gives you the ability to run a page cache warmup.",
  "keywords":["contao", "cache", "warmup", "extension", "module", "maintenance"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-cache-warmup/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-cache-warmup"
  },
  "require":{
    "php":">=7.1",
    "contao/core-bundle":"^4.0",
    "contao-community-alliance/composer-plugin":"^3.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\Contao\\PageCacheWarmupBundle\\": "src"
    }
  },
  "extra": {
    "contao-manager-plugin": "Oneup\\Contao\\PageCacheWarmupBundle\\ContaoManager\\Plugin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
